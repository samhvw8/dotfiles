{
  "env": {
    "ANTHROPIC_AUTH_TOKEN": "proxypal-local",
    "ANTHROPIC_BASE_URL": "http://127.0.0.1:8317",
    "ANTHROPIC_DEFAULT_HAIKU_MODEL": "gemini-claude-sonnet-4-5",
    "ANTHROPIC_DEFAULT_OPUS_MODEL": "gemini-claude-opus-4-5-thinking",
    "ANTHROPIC_DEFAULT_SONNET_MODEL": "gemini-claude-sonnet-4-5-thinking",
    "ANTHROPIC_MODEL": "gemini-claude-opus-4-5-thinking"
  },
  "permissions": {
    "allow": [
      "Bash(cat:*)",
      "Bash(chmod:*)",
      "Bash(curl:*)",
      "Bash(echo:*)",
      "Bash(git:*)",
      "Bash(go:*)",
      "Bash(head:*)",
      "Bash(ls:*)",
      "Bash(lsof:*)",
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(npm run:*)",
      "Bash(npm:*)",
      "Bash(npx:*)",
      "Bash(python3:*)",
      "Bash(rg:*)",
      "Bash(sed:*)",
      "Bash(source:*)",
      "Bash(tail:*)",
      "Read(~/.zshrc)",
      "Skill(prompt-enhancer)",
      "Skill(skill-developer)",
      "Update(*:*)",
      "WebFetch(domain:*)",
      "WebSearch",
      "Write(*)",
      "mcp__chrome-mcp-server",
      "mcp__context7"
    ],
    "deny": []
  },
  "model": "opus",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "bash $HOME/.claude/hooks/session-start.sh",
            "timeout": 5
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "WebSearch",
        "hooks": [
          {
            "type": "command",
            "command": "node $HOME/.claude/web_search/index.cjs",
            "timeout": 300
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node $HOME/.claude/hooks/find-delegation.cjs",
            "timeout": 120
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "node $(ls -td $HOME/.claude/plugins/cache/claude-hud/claude-hud/*/ 2>/dev/null | head -1)dist/index.js"
  },
  "enabledPlugins": {
    "claude-hud@claude-hud": true,
    "context7@claude-plugins-official": true
  },
  "alwaysThinkingEnabled": true,
  "attribution": {
    "commit": "",
    "pr": ""
  }
}
