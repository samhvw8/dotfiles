{
  "alwaysThinkingEnabled": true,
  "enabledPlugins": {
    "context7@claude-plugins-official": true,
    "mind@memvid": true
  },
  "env": {
    "ANTHROPIC_AUTH_TOKEN": "proxypal-local",
    "ANTHROPIC_BASE_URL": "http://127.0.0.1:8317",
    "ANTHROPIC_DEFAULT_HAIKU_MODEL": "gemini-claude-sonnet-4-5",
    "ANTHROPIC_DEFAULT_OPUS_MODEL": "gemini-claude-opus-4-5-thinking",
    "ANTHROPIC_DEFAULT_SONNET_MODEL": "gemini-claude-sonnet-4-5-thinking",
    "ANTHROPIC_MODEL": "gemini-claude-opus-4-5-thinking"
  },
  "hooks": {
    "PreToolUse": [
      {
        "hooks": [
          {
            "command": "node $HOME/.claude/web_search/index.cjs",
            "type": "command"
          }
        ],
        "matcher": "WebSearch"
      },
      {
        "hooks": [
          {
            "command": "node $HOME/.claude/web_fetch/index.cjs",
            "type": "command"
          }
        ],
        "matcher": "WebFetch"
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "command": "bash $HOME/.claude/hooks/force-agent-skills-eval.sh",
            "type": "command"
          }
        ]
      }
    ]
  },
  "model": "opus",
  "permissions": {
    "allow": [
      "Bash(cat:*)",
      "Bash(chmod:*)",
      "Bash(curl:*)",
      "Bash(echo:*)",
      "Bash(git:*)",
      "Bash(go:*)",
      "Bash(head:*)",
      "Bash(ls:*)",
      "Bash(lsof:*)",
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(npm run:*)",
      "Bash(npm:*)",
      "Bash(npx:*)",
      "Bash(python3:*)",
      "Bash(rg:*)",
      "Bash(sed:*)",
      "Bash(source:*)",
      "Bash(tail:*)",
      "Read(~/.zshrc)",
      "Skill(prompt-enhancer)",
      "Skill(skill-developer)",
      "Update(*:*)",
      "WebFetch(domain:*)",
      "WebSearch",
      "Write(*)",
      "mcp__chrome-mcp-server",
      "mcp__context7"
    ],
    "deny": []
  }
}