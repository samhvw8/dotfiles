{
  "permissions": {
    "allow": [
      "Bash(cat:*)",
      "Bash(chmod:*)",
      "Bash(chmod:*)",
      "Bash(curl:*)",
      "Bash(echo:*)",
      "Bash(git:*)",
      "Bash(go:*)",
      "Bash(head:*)",
      "Bash(ls:*)",
      "Bash(lsof:*)",
      "Bash(mkdir:*)",
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(npm run)",
      "Bash(npm run:*)",
      "Bash(npm:*)",
      "Bash(npx:*)",
      "Bash(npx:*)",
      "Bash(python3:*)",
      "Bash(rg:*)",
      "Bash(sed:*)",
      "Bash(source:*)",
      "Bash(tail:*)",
      "Read(~/.zshrc)",
      "Skill(prompt-enhancer)",
      "Skill(skill-developer)",
      "Update(*:*)",
      "WebFetch(domain:api.github.com)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:pincc.ai)",
      "WebFetch(domain:raw.githubusercontent.com)",
      "WebFetch(domain:www.anthropic.com)",
      "WebSearch",
      "Write(*)",
      "mcp__chrome-mcp-server",
      "mcp__chrome-mcp-server__chrome_get_web_content",
      "mcp__chrome-mcp-server__chrome_navigate",
      "mcp__context7"
    ],
    "deny": []
  },
  "hooks": {
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "bash $HOME/.claude/hooks/force-agent-skills-eval.sh"
          }
        ]
      }
    ]
  },
  "alwaysThinkingEnabled": true
}
